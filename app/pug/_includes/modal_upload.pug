#upload_files.modal.fade(tabindex='-1', role='dialog')
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        button.close(type='button', data-dismiss='modal', aria-label='Close')
          span(aria-hidden='true') &times;
        h4.modal-title Upload Images/Documents
      .modal-body
        .text-center
          ul.nav.nav-tabs(role='tablist')
            li.active(role='presentation')
              a(href='#ortho_set', aria-controls='home', role='tab', data-toggle='tab') Ortho Set
            li(role='presentation')
              a(href='#panorex', aria-controls='profile', role='tab', data-toggle='tab') Panorex
            li(role='presentation')
              a(href='#lateral_cef', aria-controls='messages', role='tab', data-toggle='tab') Lateral CEF
            li(role='presentation')
              a(href='#intra_oral', aria-controls='settings', role='tab', data-toggle='tab') Intra-Oral Radiographs
            li(role='presentation')
              a(href='#misc_photos', aria-controls='settings', role='tab', data-toggle='tab') Misc Photos/Radiographs
            li(role='presentation')
              a(href='#documents', aria-controls='settings', role='tab', data-toggle='tab') Documents
          .tab-content
            #ortho_set.tab-pane.active(role='tabpanel')
              p.how_to Drag & Drop Your Document Below                             
              .row
                .col-sm-4
                  .panel
                    .panel-heading
                      | Facial Profile
                      i.fa.fa-info-circle(aria-hidden="true")
                    .panel-body
                      img(src="images/helper_facial_profile.png", alt="Facial profile") 
                    .panel-footer                    
                      .form-group
                        .select-date.input-group.date
