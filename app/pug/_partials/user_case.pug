include ../_includes/navigation
main.push.container-fluid(role="main")
  .row   
    header.site-header
      ol.breadcrumb
        li
          a(href='#') Home
        li
          a(href='#') Library
        li.active Data    
      form.form-inline(action="", role="form")
        .form-group
          figure
            figcaption Patient Photo
            img(src="http://placehold.it/98x98", alt="Patient picture")            
        .form-group
          label(for="") Patient Name
          input(type="text", placeholder="Isabella Strong")
        .form-group
          label(for="") Status
          select.selectpicker(name="status")
            option(value="active") Active
        .form-group
          label(for="") Case Start Date          
          .select-date.input-group.date
            input(type='text')
            span.input-group-addon
              i.fa.fa-calendar(aria-hidden="true")
        .form-group
          label(for="") Gender 
          select.selectpicker(name="sex")
            option(value="active") Female
        .form-group
          label(for="") Case Type 
          select.selectpicker(name="case_type")
            option(value="active") Invisalign
        .form-group
          label(for="") Birthdate      
          .select-date.input-group.date
            input(type='text')
            span.input-group-addon
              i.fa.fa-calendar(aria-hidden="true")
        .form-group
          a.btn_out_pri(href="")
            i.fa.fa-plus(aria-hidden="true")
            | Add Images
          p.help-block or Documents (PDF, DOC)
        br
        br
        br
        br
        br
        br
        br        
